CONCAT(TRIM(substring([Employee Name], CHARINDEX (',',[Employee Name])+ 2,CHARINDEX ('MISS',[Employee Name]) - CHARINDEX (',',[Employee Name]) - 2)), ' ', LEFT([Employee Name],CHARINDEX (',',[Employee Name])-1)  )

