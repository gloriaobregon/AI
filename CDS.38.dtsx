ELSE CONCAT( TRIM(SUBSTRING([Employee Name], CHARINDEX (',',[Employee Name])+ 2,LEN([Employee Name]))),' ', LEFT([Employee Name],CHARINDEX (',',[Employee Name])-1)  )

